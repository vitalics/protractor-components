  "bugs": {
    "url": "https://github.com/vitalics/protractor-components/issues"
  },
  "homepage": "https://github.com/vitalics/protractor-components#readme",
  "keywords": [
    "protractor",
    "components"
  ],
  "devDependencies": {
    "@types/jasmine": "3.3.15",
    "@types/node": "12.6.8",
    "jasmine": "3.4.0",
    "protractor": "5.4.2",
    "ts-node": "8.3.0",
    "typescript": "3.5.3"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13"
  },
